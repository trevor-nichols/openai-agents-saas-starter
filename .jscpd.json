{
  "pattern": "**/*",
  "format": "typescript,tsx,javascript,jsx,python",
  "mode": "weak",
  "minTokens": 75,
  "minLines": 8,
  "reporters": ["console", "json"],
  "output": "var/reports/jscpd",
  "ignore": [
    "**/node_modules/**",
    "**/.next/**",
    "**/.turbo/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/.venv/**",
    "**/__pycache__/**",
    "**/.mypy_cache/**",
    "**/.pytest_cache/**",
    "**/.ruff_cache/**",
    "**/.artifacts/**",
    "**/var/**",
    "**/*.gen.ts",
    "apps/web-app/lib/api/client/**",
    "apps/api-service/alembic/**",
    "**/tests/**",
    "**/__tests__/**",
    "**/__stories__/**",
    "**/.storybook/**",
    "**/__snapshots__/**",
    "**/*.spec.*",
    "**/*.test.*",
    "**/fixtures/**",
    "**/mocks/**",
    "**/docs/**",
    "ops/**"
  ]
}
