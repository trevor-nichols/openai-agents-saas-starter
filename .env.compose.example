# Non-sensitive defaults consumed by Docker Compose helpers.
#
# Values here are safe to commit; developer-specific overrides belong
# in .env.local (which remains gitignored).

STARTER_LOCAL_DATABASE_MODE=compose
POSTGRES_PORT=5432
REDIS_PORT=6379
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=saas_starter_db
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/saas_starter_db
ALLOWED_HOSTS=localhost,localhost:8000,127.0.0.1,testserver,testclient
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
NEXT_PUBLIC_API_URL=http://localhost:8000
ENABLE_OTEL_COLLECTOR=false
OTEL_COLLECTOR_HTTP_PORT=4318
OTEL_COLLECTOR_GRPC_PORT=4317
OTEL_COLLECTOR_DIAG_PORT=13133
OTEL_DEBUG_VERBOSITY=detailed
ENABLE_MINIO=true
ENABLE_VAULT_DEV=false