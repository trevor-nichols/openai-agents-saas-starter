# Non-sensitive defaults consumed by Docker Compose helpers.
#
# Values here are safe to commit; developer-specific overrides belong
# in .env.local (which remains gitignored).

POSTGRES_PORT=5432
REDIS_PORT=6379
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=anything_agents
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/anything_agents
COMPOSE_PROJECT_NAME=openai-agents-starter
ALLOWED_HOSTS=localhost,localhost:8000,127.0.0.1,testserver,testclient
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
